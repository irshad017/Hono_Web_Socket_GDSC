{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
            "distDir": "out"
            }
        }
    ],
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/api" }
    ],
    "redirects": [
        { "source": "/old-url", "destination": "/new-url", "statusCode": 301 }
    ]
}
    